@model WebSystemUsers.Models.TipoModel
@{
    ViewBag.Title = "Cadastro Tipo de usuario";
}




<h2>Cadastro de Usuários</h2>

<h2>Novo Cadastro de Usuários</h2>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Lista de Tipos Usuários</h5>
                </div>
                <div class="ibox-content">

                    @using (Html.BeginForm("CadastroTipo", "Tipo"))
                    {
                        <div class="row">
                            <div class="col-lg-6">
                                @Html.LabelFor(obj => obj.nomeTipo)
                                @Html.TextBoxFor(obj => obj.nomeTipo, new { @class = "form-control" })
                            </div>
                          


                        </div>
                       
                        <div>
                            <input type="submit" value="Salvar" class="btn btn-warning" />

                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>